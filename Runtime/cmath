//=======
// cmath
//=======

#pragma once


//===========
// Namespace
//===========

namespace std {


//========
// Common
//========

inline bool isinf(float f) { return __builtin_isinf(f); }
inline bool isinf(double f) { return __builtin_isinf(f); }
inline bool isnan(float f) { return __builtin_isnan(f); }
inline bool isnan(double f) { return __builtin_isnan(f); }

}