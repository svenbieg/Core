//========
// chrono
//========

#pragma once


//===========
// Namespace
//===========

namespace std {
	namespace chrono {


//==========
// Duration
//==========

template<class _size_t>
struct duration
{
_size_t value;
};


//========
// Common
//========

inline duration<UINT> milliseconds(UINT value)
{
return { value };
}

}}
